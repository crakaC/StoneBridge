import org.example.stonebridge.data.UserType;

CREATE TABLE user (
    id INTEGER PRIMARY KEY,
    email TEXT NOT NULL,
    type TEXT AS UserType NOT NULL
);

CREATE INDEX user_email ON user(email);

selectAll:
SELECT * FROM user;

findById:
SELECT * FROM user
WHERE id = ? LIMIT 1;

findByEmail:
SELECT * FROM user
WHERE email = ?;

insertOrReplace:
INSERT OR REPLACE INTO user
VALUES ?;